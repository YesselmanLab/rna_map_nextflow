profiles {
    slurm  {
        process {
            executor = 'slurm'
            queueSize = 999
            cpus = 1
            memory = '6GB'

            withName: split_fastqs {
                memory = '64GB'
                time = '2h'
            }
        }
    }

    local {
        process {
            executor = 'local'
        }
    }
}